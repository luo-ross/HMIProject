                                        <PerspectiveCamera
                                            x:Name="PC"
                                            FarPlaneDistance="300"
                                            FieldOfView="100"
                                            LookDirection="0,0,-1"
                                            NearPlaneDistance="1"
                                            Position="0,0,300"
                                            UpDirection="0,1,0">
                                            <PerspectiveCamera.Transform>
                                                <RotateTransform3D>
                                                    <RotateTransform3D.Rotation>
                                                        <AxisAngleRotation3D
                                                            x:Name="ControlAngle"
                                                            Angle="0"
                                                            Axis="0,1,0" />
                                                    </RotateTransform3D.Rotation>
                                                </RotateTransform3D>
                                            </PerspectiveCamera.Transform>
                                        </PerspectiveCamera>
                                    </Viewport3D.Camera>
                                    <ModelVisual3D>
                                        <ModelVisual3D.Content>
                                            <Model3DGroup>
                                                <Model3DGroup.Transform>
                                                    <RotateTransform3D>
                                                        <RotateTransform3D.Rotation>
                                                            <AxisAngleRotation3D Angle="{Binding ElementName=ControlAngle, Path=Angle}" Axis="0,1,0" />
                                                        </RotateTransform3D.Rotation>
                                                    </RotateTransform3D>
                                                </Model3DGroup.Transform>
                                                <SpotLight
                                                    x:Name="L1"
                                                    InnerConeAngle="35"
                                                    Color="Transparent" />
                                                <SpotLight
                                                    x:Name="L2"
                                                    InnerConeAngle="35"
                                                    Color="Transparent" />
                                            </Model3DGroup>
                                        </ModelVisual3D.Content>

                                        <Viewport2DVisual3D>
                                            <Viewport2DVisual3D.Visual>
                                                <Button Content="test" />
                                            </Viewport2DVisual3D.Visual>

                                        </Viewport2DVisual3D>
                                    </ModelVisual3D>


