<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/lib/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/RS.WebApp.styles.css" asp-append-version="true" />
</head>
<body class="d-flex flex-column h-100">
    <header class="navbar navbar-expand-md navbar-light d-print-none header-sticky">
        <div class="container-xl">
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbar-menu">
                <span class="navbar-toggler-icon"></span>
            </button>
            <h1 class="navbar-brand navbar-brand-autodark d-none-navbar-horizontal pe-0 pe-md-3">
                @ViewData["PageName"]
            </h1>
            <div class="navbar-nav flex-row order-md-last">
                <div class="nav-item d-none d-md-flex me-3">
                    <div class="btn-list">
                        <a href="javascript:void(0);" class="btn btn-outline-white"  rel="noreferrer">
                           反馈问题
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <div class="page-wrapper">
        @RenderBody()
    </div>
    <footer class="footer  footer-transparent d-print-none mt-auto ">
        <div class="container">
            <div class="row text-center align-items-center flex-row-reverse">
                <div class="col-lg-auto ms-lg-auto">
                    <ul class="list-inline list-inline-dots mb-0">
                        <li class="list-inline-item">
                            <a href="javascript:void(0);" class="link-secondary" >文档</a>
                        </li>
                        <li class="list-inline-item">
                            <a href="javascript:void(0);" class="link-secondary" >许可证</a>
                        </li>
                        <li class="list-inline-item">
                            <a href="https://github.com/" target="_blank" class="link-secondary" rel="noopener">源码</a>
                        </li>
                    </ul>
                </div>
                <div class="col-12 col-lg-auto mt-3 mt-lg-0">
                    <ul class="list-inline list-inline-dots mb-0">
                        <li class="list-inline-item">
                            版权 © 2024
                            <a href="javascript:void(0);" class="link-secondary">与贝</a>.
                            保留所有权利.
                        </li>
                        <li class="list-inline-item">
                            <a href="http://beian.miit.gov.cn/" target="_blank">鲁ICP备12345678号</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
